<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:converter="clr-namespace:Module.AnimeSchedule.Avalonia.Converter"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Module.AnimeSchedule.Avalonia.Views.Webhooks.WebhookEditView">
  <StackPanel>
    <StackPanel Orientation="Horizontal">
      <TextBlock Text="Webhook ID"/>
      <TextBox Text="{Binding Item.Id}"/>
    </StackPanel>
    <StackPanel Orientation="Horizontal">
      <TextBlock Text="Webhook Token"/>
      <TextBox Text="{Binding Item.Token}"/>
    </StackPanel>
  </StackPanel>
</UserControl>
